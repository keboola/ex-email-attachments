{
  "name": "keboola/pigeon",
  "license": "MIT",
  "type": "project",
  "description": "Keboola Pigeon - Imports csv files in email attachments to Keboola",
  "keywords": ["keboola", "pigeon"],
  "authors": [
    {
      "name": "Jakub Matejka",
      "email": "jakub@keboola.com"
    }
  ],
  "require": {
    "php": "~7.2",
    "keboola/csv": "~1.1",
    "keboola/php-temp": "0.1.3",
    "symfony/config": "^3.2",
    "symfony/console": "^3.2",
    "symfony/filesystem": "^3.2",
    "symfony/process": "^3.2",
    "symfony/serializer": "^4.0",
    "monolog/monolog": "^1.22",
    "aws/aws-sdk-php": "^3.46"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.1",
    "squizlabs/php_codesniffer": "^2.8",
    "phpstan/phpstan": "^0.6.4"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Keboola\\Pigeon\\": "src/Keboola/Pigeon/",
      "Keboola\\Pigeon\\Tests\\": "tests/Keboola/Pigeon/"
    }
  }
}
